{
  "manifest_version": 2,

  "name": "What's my zip?",
  "description": "Find out where the heck you are.",
  "version": "1.0.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "dist/index.html"
  },
  "permissions": [
    "clipboardWrite",
    "geolocation",
    "http://api.geonames.org/"
   ]
}