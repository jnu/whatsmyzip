{
  "name": "whatsmyzip",
  "version": "1.0.0",
  "description": "What's my zip code?",
  "main": "src/js/main.js",
  "scripts": {
    "test": "$(npm bin)/eslint ./src/js",
    "clean": "rm -rf ./dist",
    "build-html": "mkdir -p ./dist && cp ./src/html/popup.html ./dist/",
    "build": "npm run clean && npm run build-html && $(npm bin)/webpack",
    "start": "npm run clean && npm run build-html && $(npm bin)/webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jnu/whatsmyzip.git"
  },
  "keywords": [
    "zipcode",
    "zip",
    "chrome",
    "extension",
    "geo",
    "geolocation"
  ],
  "author": "Joe Nudell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jnu/whatsmyzip/issues"
  },
  "homepage": "https://github.com/jnu/whatsmyzip",
  "devDependencies": {
    "babel-core": "^5.8.5",
    "babel-loader": "^5.3.2",
    "eslint": "^0.24.1",
    "webpack": "^1.10.5"
  }
}
